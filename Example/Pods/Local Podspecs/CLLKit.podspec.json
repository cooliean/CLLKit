{
  "name": "CLLKit",
  "version": "1.0.0",
  "summary": "Powerful IOS Extension",
  "description": "尝试一下封装一个自己的合适的框架，慢慢的积累一下",
  "homepage": "https://github.com/cooliean/CLLKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Cooliean": "cooliean@qq.com"
  },
  "source": {
    "git": "https://github.com/cooliean/CLLKit.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "frameworks": [
    "CoreFoundation",
    "Foundation",
    "CoreGraphics",
    "UIKit"
  ],
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "CLLKit/Classes/**/*",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "CLLKit/Classes/Core/**/*.{h,m}",
      "public_header_files": "CLLKit/Classes/Core/**/*.h",
      "dependencies": {
        "YTKNetwork": [
          "~> 1.1.0"
        ],
        "IQKeyboardManager": [
          "~> 4.0.3"
        ],
        "DZNEmptyDataSet": [
          "~> 1.8.1"
        ],
        "SDAutoLayout": [
          "~> 2.1.1"
        ],
        "XLForm": [
          "~> 3.1.2"
        ],
        "MJExtension": [
          "~> 3.0.10"
        ],
        "MJRefresh": [
          "~> 3.1.0"
        ],
        "JVFloatLabeledTextField": [
          "1.1.0"
        ],
        "SVProgressHUD": [
          "~> 2.0.3"
        ],
        "MXSegmentedPager": [
          "~> 3.0.4"
        ]
      }
    },
    {
      "name": "CommonTools",
      "source_files": "CLLKit/Classes/CommonTools/**/*.{h,m}",
      "public_header_files": "CLLKit/Classes/CommonTools/**/*.h"
    }
  ]
}
